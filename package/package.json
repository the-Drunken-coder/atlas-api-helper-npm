{
  "name": "@atlasnpm/atlas-api-helper",
  "version": "0.2.18",
  "type": "module",
  "description": "HTTP client for the Atlas Command REST API.",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "build": "npx tsup",
    "test": "vitest run",
    "prepare": "node ./scripts/prepare.mjs"
  },
  "dependencies": {
    "@types/node": "^24.10.1",
    "tsup": "^8.5.1",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=18"
  }
}
